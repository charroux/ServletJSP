<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
<display-name>Data</display-name>
		
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>firstServlet</servlet-name>
		<servlet-class>tp1.PremiereServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>firstServlet</servlet-name>
		<url-pattern>/urlPremiereServlet</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>firstJSP</servlet-name>
		<servlet-class>tp1.PremiereServletJSP</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>firstJSP</servlet-name>
		<url-pattern>/urlServletForm</url-pattern>
	</servlet-mapping>
	
	

	<servlet>
		<servlet-name>JSPBean</servlet-name>
		<servlet-class>tp1.ServletJSPBean</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>JSPBean</servlet-name>
		<url-pattern>/urlServletBean</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>NbPersonnes</servlet-name>
		<servlet-class>tp1.ServletNbPersonnes</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>NbPersonnes</servlet-name>
		<url-pattern>/urlNbPersonnes</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>Personnes</servlet-name>
		<servlet-class>tp1.ServletPersonnes</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Personnes</servlet-name>
		<url-pattern>/urlPersonnes</url-pattern>
	</servlet-mapping>
			
</web-app>